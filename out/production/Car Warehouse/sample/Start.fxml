<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StartController">
   <left>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <AnchorPane prefHeight="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="41.0" layoutY="154.0" prefHeight="1.0" prefWidth="100.0" text="Continue as " wrapText="true">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button layoutX="162.0" layoutY="155.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="101.0" text="Manufacturer"
                    onAction="#onManufacturerButtonPressed">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button layoutX="162.0" layoutY="200.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="101.0" text="Viewer"
                    onAction="#onViewerButtonPressed">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button layoutX="351.0" layoutY="361.0" mnemonicParsing="false" text="Exit" textFill="#ff0033" wrapText="true"
                    onAction="#onExitButtonPressed"/>
         </children></AnchorPane>
   </right>
</BorderPane>
