<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.StartController">
   <left>
      <AnchorPane prefHeight="400.0" prefWidth="256.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="74.0" layoutY="98.0" text="Maruf's">
               <font>
                  <Font name="Century Gothic Bold" size="42.0" />
               </font>
            </Label>
            <Label layoutX="109.0" layoutY="152.0" prefHeight="54.0" prefWidth="81.0" text="Car">
               <font>
                  <Font name="Century Gothic Bold" size="42.0" />
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="206.0" text="Warehouse">
               <font>
                  <Font name="Century Gothic Bold" size="42.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </left>
   <right>
      <AnchorPane prefHeight="400.0" prefWidth="340.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="123.0" layoutY="97.0" prefHeight="1.0" prefWidth="100.0" text="Continue as " wrapText="true">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button layoutX="109.0" layoutY="150.0" mnemonicParsing="false" onAction="#onManufacturerButtonPressed" prefHeight="28.0" prefWidth="123.0" text="Manufacturer" textAlignment="CENTER">
               <font>
                  <Font name="Lucida Fax Demibold" size="14.0" />
               </font>
            </Button>
            <Button contentDisplay="CENTER" layoutX="123.0" layoutY="200.0" mnemonicParsing="false" onAction="#onViewerButtonPressed" prefHeight="25.0" prefWidth="101.0" text="Viewer" textAlignment="CENTER">
               <font>
                  <Font name="Lucida Fax Demibold" size="14.0" />
               </font>
            </Button>
            <Button layoutX="283.0" layoutY="365.0" mnemonicParsing="false" onAction="#onExitButtonPressed" prefHeight="23.0" prefWidth="42.0" text="Exit" textFill="RED" wrapText="true">
               <font>
                  <Font name="Arial" size="12.0" />
               </font></Button>
         </children></AnchorPane>
   </right>
</BorderPane>
