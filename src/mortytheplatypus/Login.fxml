<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="mortytheplatypus.LoginController">
   <left>
      <AnchorPane prefHeight="400.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane prefHeight="400.0" prefWidth="256.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" layoutX="74.0" layoutY="98.0" text="Maruf's">
                     <font>
                        <Font name="Century Gothic Bold" size="42.0" />
                     </font>
                  </Label>
                  <Label layoutX="109.0" layoutY="152.0" prefHeight="54.0" prefWidth="81.0" text="Car">
                     <font>
                        <Font name="Century Gothic Bold" size="42.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="206.0" text="Warehouse">
                     <font>
                        <Font name="Century Gothic Bold" size="42.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children></AnchorPane>
   </left>
   <right>
      <AnchorPane prefHeight="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="29.0" layoutY="123.0" text="Username">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="29.0" layoutY="157.0" text="Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="usernameField" layoutX="117.0" layoutY="123.0" onKeyReleased="#handleKeyReleased" prefHeight="25.0" prefWidth="245.0" />
            <PasswordField fx:id="passwordField" layoutX="117.0" layoutY="157.0" onKeyReleased="#handleKeyReleased" prefHeight="25.0" prefWidth="245.0" />
            <Button fx:id="loginButton" layoutX="144.0" layoutY="229.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" prefHeight="26.0" prefWidth="176.0" text="Login" />
            <Button fx:id="cancelButton" layoutX="144.0" layoutY="268.0" mnemonicParsing="false" onAction="#onCancelButtonPressed" prefHeight="26.0" prefWidth="176.0" text="Cancel" />
            <Label fx:id="warning" layoutX="116.0" layoutY="200.0" prefHeight="17.0" prefWidth="247.0" textAlignment="CENTER" textFill="RED" />
            <Button layoutX="253.0" layoutY="361.0" lineSpacing="10.0" mnemonicParsing="false" onAction="#onReturnToMainPage" prefHeight="25.0" prefWidth="134.0" text="Return to Main page" />
            <Label layoutX="127.0" layoutY="45.0" prefHeight="56.0" prefWidth="211.0" text="Manufacturer Login">
               <font>
                  <Font name="Century Gothic" size="22.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </right>
</BorderPane>
